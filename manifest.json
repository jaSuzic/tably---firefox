{
    "manifest_version": 2,
    "name": "Tably - New Tab addon",
    "version": "1.1.1",
    "chrome_url_overrides": {
        "newtab": "new-tab.html"
    },
    "icons": {"64": "./assets/images/favicon.png"},
    "permissions": ["storage"],
    "web_accessible_resources": [
        "/assets/*"
    ]
}